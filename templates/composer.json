{
	"name": "madrasthemes/mas-elementor",
	"description": "More power to your Elementor powered website with beautifully designed sections, templates, widgets, skins and extensions.",
	"type": "project",
	"license": "GNU General Public License v3.0",
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"inewlegend/elementor-editor-testing": "0.0.1",
		"justinrainbow/json-schema": "^5.2",
		"phpunit/phpunit": "^5|^6|^7",
		"squizlabs/php_codesniffer": "^3.6",
		"wp-coding-standards/wpcs": "^2.3",
		"yoast/phpunit-polyfills": "^1.0",
        "woocommerce/woocommerce-sniffs": "0.1.1",
		"phpcompatibility/php-compatibility": "9.3.5",
		"woocommerce/woocommerce-git-hooks": "1.0.5"
	},
	"scripts": {
		"lint": "phpcs --standard=phpcs.xml",
		"test": "phpunit",
		"test:install": "bash ./bin/install-wp-tests-local.sh",
        "pre-update-cmd": [
			"WooCommerce\\GitHooks\\Hooks::preHooks"
		],
		"pre-install-cmd": [
			"WooCommerce\\GitHooks\\Hooks::preHooks"
		],
		"post-install-cmd": [
			"WooCommerce\\GitHooks\\Hooks::postHooks"
		],
		"post-update-cmd": [
			"WooCommerce\\GitHooks\\Hooks::postHooks"
		],
		"phpcs": [
			"phpcs --extensions=php -s -p"
		],
		"phpcbf": [
			"phpcbf --extensions=php -p"
		]
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
